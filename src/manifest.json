{
  "name": "Cibus Calc",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "img/icon-48.png"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "cookies",
    "*://*.mysodexo.co.il/*"
  ]
}
